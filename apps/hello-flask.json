{
  "id": "/hello-flask",
  "cmd": null,
  "cpus": 1,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "r4um/hello-flask",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
